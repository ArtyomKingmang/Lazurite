using "lzr.utils.arrays"

arr = [10, 7, 8, 2, 5, 4, 1, 3, 6, 9]

for(i = 0, i < 10, i++) {
    for(j = 0, j < 9, j++) {
        if (arr[j] > arr[j + 1]) {
            n = arr[j]
            arr[j] = arr[j + 1]
            arr[j + 1] = n
        }
    }
}

for(i = 0, i < 10, i++) {
    print(str(arr[i]) + " ")
}