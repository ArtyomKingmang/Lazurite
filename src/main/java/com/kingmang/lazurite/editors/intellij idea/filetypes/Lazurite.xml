<filetype binary="false" description="Lazurite files" name="Lazurite">
  <highlighting>
    <options>
      <option name="LINE_COMMENT" value="//" />
      <option name="COMMENT_START" value="/*" />
      <option name="COMMENT_END" value="*/" />
      <option name="HEX_PREFIX" value="0x" />
      <option name="NUM_POSTFIXES" value="" />
      <option name="HAS_BRACES" value="true" />
      <option name="HAS_BRACKETS" value="true" />
      <option name="HAS_PARENS" value="true" />
      <option name="HAS_STRING_ESCAPES" value="true" />
    </options>
    <keywords keywords="do;return;break;case;continue;echo;else;filter;for;foreach;getBytes;if;length;match;print;println;range;readln;sprintf;substring;throw;while" ignore_case="false" />
    <keywords2 keywords="Array;try;catch;macro;func" />
    <keywords3 keywords="false;this;true;null" />
    <keywords4 keywords="include;using;define;new;class;enum" />
  </highlighting>
  <extensionMap>
    <mapping ext="lzr" />
  </extensionMap>
</filetype>
